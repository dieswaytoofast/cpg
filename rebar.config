{deps,
 [{trie, ".*",
   {git, "git://github.com/okeuday/trie", {branch, "master"}}},
  {quickrand, ".*",
   {git, "git://github.com/okeuday/quickrand", {branch, "master"}}},
  {proper, ".*",
   {git, "git://github.com/manopapad/proper", {tag, "v1.1"}}},
  {reltool_util, ".*",
   {git, "git://github.com/okeuday/reltool_util", {branch, "master"}}}]}.

{erl_opts, [warnings_as_errors,
            strict_validation,
            warn_bif_clash,
            warn_deprecated_function,
            warn_export_all,
            warn_export_vars,
            warn_exported_vars,
            warn_obsolete_guard,
            warn_shadow_vars,
            warn_unused_import,
            warn_unused_function,
            warn_unused_record,
            warn_unused_vars]}.
{xref_checks, [undefined_function_calls]}.
{cover_enabled, true}.
{cover_print_enabled, true}.
{cover_export_enabled, true}.
{ct_extra_params, "-spec test/ct.spec"}.
